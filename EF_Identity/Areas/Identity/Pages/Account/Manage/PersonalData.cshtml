@page "/user-2fa"
@model PersonalDataModel
@{
    ViewData["Title"] = "Du lieu ca nhan";
    ViewData["ActivePage"] = ManageNavPages.PersonalData;
}

<h4>@ViewData["Title"]</h4>

<div class="row">
    <div class="col-md-6">Ban co the tai thong tin ca nhan ve hoac xoa hoan toan tai khoan ca nhan tren razor web.</p>
        <p>
            <strong>Neu xoa tai khoan thi khong the phuc hoi.</strong>
        </p>
        <form id="download-data" asp-page="DownloadPersonalData" method="post" class="form-group">
            <button class="btn btn-primary" type="submit">Tai du lieu</button>
        </form>
        <p>
            <a id="delete" asp-page="DeletePersonalData" class="btn btn-secondary">Xoa tai khoan</a>
        </p>
    </div>
</div>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
